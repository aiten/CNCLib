<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
    <PublishProvider>AzureWebSite</PublishProvider>
    <LastUsedBuildConfiguration>Debug</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://cnclibapi.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>cnclibapi.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>CNCLibAPI</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$CNCLibAPI</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="CNCLibContext" Order="1" Enabled="False">
          <Destination Path="Data Source=cnclibdb.database.windows.net;Initial Catalog=CNCLibDb;Persist Security Info=True;User ID=Herbert;Password=Edith1234" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="CNCLib.Repository.Context.CNCLibContext, CNCLib.Repository" MigrationConfiguration="CNCLib.Repository.Migrations.Configuration, CNCLib.Repository" Origin="Configuration" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="CNCLibContextAzure" Order="2" Enabled="False">
          <Destination Path="" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=cnclibdb.database.windows.net;Initial Catalog=CNCLibDb;Persist Security Info=True;User ID=Herbert;Password=Edith1234" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\CNCLibContextAzure_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="Data Source=cnclibdb.database.windows.net;Initial Catalog=CNCLibDb;Persist Security Info=True;User ID=Herbert;Password=Edith1234;" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)CNCLibContext-Web.config Connection String">
      <ParameterValue>Data Source=cnclibdb.database.windows.net;Initial Catalog=CNCLibDb;Persist Security Info=True;User ID=Herbert;Password=Edith1234</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>